
#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c, D, x1, x2;

    printf("Enter a, b and c: ");
    scanf("%f %f %f", &a, &b, &c);

    if (a == 0) {
        printf("Not a quadratic equation");
    } else {
        D = b*b - 4*a*c;

        if (D > 0) {
            x1 = (-b + sqrt(D)) / (2*a);
            x2 = (-b - sqrt(D)) / (2*a);
            printf("Two real roots: %f and %f", x1, x2);
        }
        else if (D == 0) {
            x1 = -b / (2*a);
            printf("One real root: %f", x1);
        }
        else {
            printf("No real roots");
        }
    }

   
}
