#include <stdio.h>
#include <math.h>

void isprime(int num) {
    int n=num;
    int i,count=0;
    for(i=2;i<=num/2;i++){
        if(num%i==0){
            count=1;
        }
        
        
    }
    if (count==0){
        printf("%d",n);
    }
    
    
    
}

int main() {
    int start, end;

    printf("Enter start number: ");
    scanf("%d", &start);

    printf("Enter end number: ");
    scanf("%d", &end);

    printf("prime numbers between %d and %d are:\n", start, end);

    for (int i = start; i <= end; i++) {
       isprime(i);
    }

    return 0;
}
